<?xml version="1.0" ?>

<PROPERTY_LIST>

    <!-- Simulation time ratio to real time -->
    <!-- Default: 1000 -->
    <TIME_RATIO>1000</TIME_RATIO>

    <!-- Airlock pressurization/depressurization time (millisols) -->
    <!-- Default: 10.0 millisols -->
    <AIRLOCK_CYCLE_TIME>10.0</AIRLOCK_CYCLE_TIME>

    <PERSON_PROPERTIES>

        <!-- The average amount of oxygen (kg) breathed by a person each Martian Sol -->
        <!-- Default: 1.0 -->
        <OXYGEN_CONSUMPTION>1.0</OXYGEN_CONSUMPTION>

        <!-- Number of Earth minutes a person can survive without Oxygen -->
        <!-- Default: 5.0 -->
        <LACK_OF_OXYGEN>5.0</LACK_OF_OXYGEN>

        <!-- The average amount of water (kg) drank by a person each Martian Sol -->
        <!-- Default: 4.0 -->
        <WATER_CONSUMPTION>4.0</WATER_CONSUMPTION>

        <!-- Number of Earth minutes a person can survive without Water -->
        <!-- Default: 8640, 6 days -->
        <LACK_OF_WATER>8640.0</LACK_OF_WATER>

        <!-- The average amount of food (kg) eaten by a person each Martian Sol -->
        <!-- Default: 1.5 -->
        <FOOD_CONSUMPTION>1.5</FOOD_CONSUMPTION>

        <!-- Number of Earth minutes a person can survive without Food -->
        <!-- Default: 30240.0, 21 days -->
        <LACK_OF_FOOD>30240.0</LACK_OF_FOOD>

	<!-- Minimum air pressure (atm) a person can withstand -->
	<!-- Default: .25 atm -->
	<MIN_AIR_PRESSURE>.25</MIN_AIR_PRESSURE>

	<!-- Number of Earth seconds a person can survive in decompression -->
	<!-- Default: 90.0 seconds -->
	<DECOMPRESSION>90.0</DECOMPRESSION>

	<!-- Minimum temperature (degrees celsius) a person can withstand -->
	<!-- Default: 0 degrees celsius -->
	<MIN_TEMPERATURE>0.0</MIN_TEMPERATURE>

        <!-- Maximum temperature (degrees celsius) a person can withstand -->
	<!-- Default: 48 degrees celsius -->
	<MAX_TEMPERATURE>48.0</MAX_TEMPERATURE>
	
	<!-- Number of Earth minutes a person can survive below minimum temperature -->
	<!-- Default: 240 minutes (4 hours) -->
	<FREEZING_TIME>240.0</FREEZING_TIME>
	
    </PERSON_PROPERTIES>

    <ROVER_PROPERTIES>

        <!-- The average kilometers/kilogram fuel efficiency of a rover -->
        <!-- Default: 2.0 -->
        <FUEL_EFFICIENCY>2.0</FUEL_EFFICIENCY>

        <!-- The range of the rover (km) -->
        <!-- Default: 4000.0 -->
        <RANGE>4000</RANGE>

    </ROVER_PROPERTIES>

    <SETTLEMENT_PROPERTIES>

        <!-- The amount of oxygen (kg) that can be stored in a settlement -->
        <!-- Default: 10000.0 -->
        <OXYGEN_STORAGE_CAPACITY>10000.0</OXYGEN_STORAGE_CAPACITY>

        <!-- The amount of water (kg) that can be stored in a settlement -->
        <!-- Default: 10000.0 -->
        <WATER_STORAGE_CAPACITY>10000.0</WATER_STORAGE_CAPACITY>

        <!-- The amount of food (kg) that can be stored in a settlement -->
        <!-- Default: 10000.0 -->
        <FOOD_STORAGE_CAPACITY>10000.0</FOOD_STORAGE_CAPACITY>

        <!-- The amount of fuel (kg) that can be stored in a settlement -->
        <!-- Default: 10000.0 -->
        <FUEL_STORAGE_CAPACITY>10000.0</FUEL_STORAGE_CAPACITY>

        <!-- The amount of food (kg) in a full green house harvest -->
        <!-- Default: 200.0 -->
        <GREENHOUSE_FULL_HARVEST>200.0</GREENHOUSE_FULL_HARVEST>

        <!-- The length of time (millisols) in the greenhouse growing cycle -->
        <!-- Default: 10000.0 -->
        <GREENHOUSE_GROWING_CYCLE>10000.0</GREENHOUSE_GROWING_CYCLE>

    </SETTLEMENT_PROPERTIES>

    <INIT_PROPERTIES>
        <INIT_SETTLEMENTS>10</INIT_SETTLEMENTS>
    </INIT_PROPERTIES>
</PROPERTY_LIST>
