	<!-- ************************************************************************************************* -->
	<!-- * Project Tools Section                                                                         * -->
	<!-- ************************************************************************************************* -->
	<scm>
		<connection>scm:svn:https://mars-sim.svn.sourceforge.net/svnroot/mars-sim/tags/mars-sim-2.90</connection>
		<developerConnection>scm:svn:https://mars-sim.svn.sourceforge.net/svnroot/mars-sim/tags/mars-sim-2.90</developerConnection>
		<url>http://mars-sim.svn.sourceforge.net/viewvc/mars-sim/tags/mars-sim-2.90</url>
	</scm>
	<issueManagement>
		<system>sourceforge</system>
		<url>http://sourceforge.net/apps/mantisbt/mars-sim/</url>
	</issueManagement>
	<mailingLists>
		<mailingList>
			<name>Mars-Sim Development Mailing list</name>
			<subscribe>
    			http://lists.sourceforge.net/lists/listinfo/mars-sim-developers
    		</subscribe>
			<unsubscribe>
    			http://lists.sourceforge.net/lists/listinfo/mars-sim-developers
